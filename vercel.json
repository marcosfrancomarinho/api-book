{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@now/node",
            "config": {
                "includesFiles": [
                    "db.json"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.js"
        }
    ]
}